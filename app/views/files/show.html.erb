<br>
<div class="file-container">
  <div class="row">
    <div class="col-md-9">
      <div class="panel panel-default">
        <div class="panel-body">
          <div class="row">
            <div class="col-md-12 lead">File Details<hr></div>
          </div>
          <div class="row">
			<div class="col-md-4 text-center">
              <img class="img-circle avatar avatar-original" style="-webkit-user-select:none; 
              display:block; margin:auto;" src="http://icons.iconarchive.com/icons/dtafalonso/android-lollipop/512/Docs-icon.png" width="120" height="120">
            </div>
            <div class="col-md-8">
              <div class="row">
                <div class="col-md-12">
                  <h1 class="only-bottom-margin"><%=@file.name%></h1>
                </div>
              </div>
              <div class="row">
                <div class="col-md-6">
                  <span class="text-muted">Title:</span> <br>
                  <span class="text-muted">Created On:</span> <br>
                  <span class="text-muted">Employee Id:</span> <br>
                  <span class="text-muted">Size:</span> <br>
                </div>
                <div class="col-md-6">
                  <div class="activity-mini">
                    <i class="glyphicon glyphicon-comment text-muted"></i> <%=@file.title%>
                  </div>
                  <div class="activity-mini">
                    <i class="glyphicon glyphicon-thumbs-up text-muted"></i> <%=@file.created_on%>
                  </div>
                  <div class="activity-mini">
                    <i class="glyphicon glyphicon-thumbs-up text-muted"></i> <%=@file.employee_id%>
                  </div>
                  <div class="activity-mini">
                    <i class="glyphicon glyphicon-thumbs-up text-muted"></i> <%=@file.size%>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-md-12">
              <a href='/'>
                <button class="btn btn-default pull-right">
                	<i class="glyphicon glyphicon-pencil"></i> Back
                </button>
              </a>  
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>